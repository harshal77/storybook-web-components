            "rawdescription": "\n",
            "type": "component",
            "sourceCode": "import { Component, Input, Output, EventEmitter } from '@angular/core';\n\n@Component({\n\tselector: 'app-button',\n\ttemplateUrl: './button.component.html',\n\tstyleUrls: ['./button.component.scss'],\n})\nexport class ButtonComponent {\n\t@Input() title: string = 'Primary Button';\n\t@Input() backgroundColor: string = '#660099';\n\t@Input() displayIcon: boolean = false;\n\t@Input() roundedButton: boolean = true;\n\t@Input() iconLink: string = 'home';\n\t@Input() buttonFontSize: string = '1.5rem';\n\t@Input() buttonHeight: string = '3rem';\n\t@Input() textColor: string = 'white';\n\t@Input() isDisabled: boolean = false;\n\n\t@Output() buttonClick = new EventEmitter<any>();\n\t\n\tbuttonClicked() {\n\t\tthis.buttonClick.emit();\n\t}\n}\n",
            "assetsDirs": [],
            "styleUrlsData": [
                {
                    "data": "button {\n  width: 100%;\n  text-align: center;\n  font-size: 1rem;\n  border: none;\n}\n\n.rounded {\n  border-radius: 2rem;\n}\n\n.center {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.margin-5 {\n  margin-right: 5px;\n}\n\n.disabled {\n  opacity: 0.4;\n}\n",
                    "styleUrl": "./button.component.scss"
                }
            ],
            "stylesData": "",
            "templateData": "<button class=\"center\" \n  [ngStyle]=\"{ 'background-color': backgroundColor, 'color':textColor, 'height':buttonHeight}\" \n  [ngClass]=\"{ rounded: roundedButton, pointer: !isDisabled, disabled: isDisabled}\" \n  (click)=\"buttonClicked()\" \n  [disabled]=\"isDisabled\">\n  <mat-icon class=\"margin-5\" *ngIf=\"displayIcon\">{{iconLink}}</mat-icon>\n  <span [ngStyle]=\"{ 'font-size': buttonFontSize }\">{{title}}</span>\n</button>\n"
        }
    ],
